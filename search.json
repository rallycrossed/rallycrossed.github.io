        "content"   : "<section class=\"section--postsWrapper\">\n  <div class=\"container\">\n    <div class=\"blockGroup\">\n      <ul class=\"blockGroup-list\">\n\n        {% for post in paginator.posts %}\n\n        {% assign post = site.posts.first %}\n        {% assign content = post.content %}\n\n        <li class=\"block\">\n          <div class=\"postArticle-wrapper\">\n\n            <div class=\"post-preview\"\n              style=\"border-bottom-style: solid; border-bottom-width: 1px; border-bottom-color: #530720;\">\n\n              <a href=\"{{ post.url | prepend: site.baseurl }}\">\n                {% if post.image.feature %}\n                {% picture blogcrop {{ post.image.feature }} --alt {{ post.title }} %}\n                {% endif %}\n                <p class=\"post-title\"\n                  style=\"font-family: Open Sans, Helvetica Neue, Helvetica, Arial, sans-serif; \">\n                  {{ post.title }}\n                </p>\n              </a>\n              {% if post.subtitle %}\n              <p class=\"post-subtitle\"\n                style=\"font-family: Open Sans, Helvetica Neue, Helvetica, Arial, sans-serif; \">\n                {{ post.subtitle }}</p>\n              {% endif %}\n\n              <p class=\"post-meta\"\n                style=\"font-family: Open Sans, Helvetica Neue, Helvetica, Arial, sans-serif; \">\n                Posted on {{ post.date | date: \"%B %-d, %Y\" }}</p>\n            </div>\n\n          </div>\n        </li>\n        {% endfor %}\n        <p>&nbsp;</p>\n      </ul>\n    </div>\n  </div>\n</section>\n\n\n<!-- Pager -->\n{% if paginator.total_pages > 1 %}\n<ul class=\"pager\" style=\"font-family: Open Sans, Helvetica Neue, Helvetica, Arial, sans-serif;\">\n  {% if paginator.previous_page %}\n  <li class=\"previous\">\n    <a href=\"{{ paginator.previous_page_path | prepend: site.baseurl | replace: '//', '/' }}\">&larr; Newer Posts</a>\n  </li>\n  {% endif %}\n\n  <span class=\"button-height\"> Page {{paginator.page}} of {{paginator.total_pages}}</span>\n\n  {% if paginator.next_page %}\n  <li class=\"next\">\n    <a href=\"{{ paginator.next_page_path | prepend: site.baseurl | replace: '//', '/' }}\">Older Posts &rarr;</a>\n  </li>\n  {% endif %}\n</ul>\n{% endif %}\n\n<br>"
     
